{
    "manifest_version": 3,
    "name": "Tab Session Manager",
    "version": "1.0",
    "permissions": ["tabs", "storage", "windows"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }